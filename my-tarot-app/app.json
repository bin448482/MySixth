{
  "expo": {
    "name": "my-tarot-app",
    "slug": "my-tarot-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mytarotapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#5B4A8A",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 240,
          "resizeMode": "contain",
          "backgroundColor": "#0a0a1a",
          "dark": {
            "image": "./assets/images/icon.png",
            "backgroundColor": "#0a0a1a"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "tabsConfig": {
        "tabBarEnabled": false,
        "tabs": {
          "home": {
            "enabled": true,
            "title": "Home",
            "icon": "house.fill"
          },
          "explore": {
            "enabled": true,
            "title": "Explore",
            "icon": "paperplane.fill"
          }
        }
      },
      "router": {},
      "eas": {
        "projectId": "0d19ed54-5efd-4fe1-ae51-23a1b6602215"
      }
    },
    "assetBundlePatterns": [
      "assets/images/**/*",
      "assets/db/**/*",
      "assets/data/**/*"
    ]
  }
}